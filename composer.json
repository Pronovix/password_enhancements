{
    "name": "drupal/password_enhancements",
    "type": "drupal-module",
    "homepage": "https://github.com/Pronovix/password_enhancements",
    "description": "Password related enhancements.",
    "keywords": ["Drupal", "password", "enhancements", "security"],
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal/core": "^8.7",
        "ext-mbstring": "*",
        "php": ">=7.1"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "behat/mink": "dev-master#153e4bb",
        "drupal/drupal-extension": "^3.4.0",
        "instaclick/php-webdriver": "^1.4.5",
        "phpunit/phpunit": "^6.5",
        "webflo/drupal-core-require-dev": "^8.7"
    },
    "support": {
        "issues": "https://github.com/Pronovix/password_enhancements/issues",
        "source": "https://github.com/Pronovix/password_enhancements"
    },
    "autoload": {
        "psr-4": {
            "Drupal\\password_enhancements\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\password_enhancements\\": "./tests/"
        }
    }
}
